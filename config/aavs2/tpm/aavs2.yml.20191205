        integrated_data_ip: "10.0.10.200"
        use_teng_integrated: False

    csp_ingest:
        dst_mac: 0x98039b86158e
        dst_ip: "10.0.10.210"
        dst_port: 4660
        src_mac: Null
        src_ip: 10.0.10.254
